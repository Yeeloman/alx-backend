#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules/kue/bin/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules/kue/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules/kue/bin/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules/kue/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/kue@0.11.6/node_modules:/home/yeeloman/studies/alx-backend/0x03-queuing_system_in_js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../kue/bin/kue-dashboard" "$@"
else
  exec node  "$basedir/../kue/bin/kue-dashboard" "$@"
fi
